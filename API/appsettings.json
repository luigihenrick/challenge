{
  "HackerNews":{
    "BaseUri": "https://hacker-news.firebaseio.com/",
    "Polly": {
      "MaxRetryCount": 3,
      "SleepDurationInSeconds": 5
    },
    "Cache": {
      "AbsoluteExpiration": 360,
      "SlidingExpiration": 5
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
